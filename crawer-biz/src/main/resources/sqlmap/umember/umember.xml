    <select id="getByParams" parameterType="com.mk.crawer.biz.module.UMember" resultMap="u_member" >
        SELECT * FROM  ots.u_member where 1=1
        <if test="mid != null">
            AND mid = #{mid}
        </if>
        <if test="phone != null">
            AND phone = #{phone}
        </if>
        <if test="idcard != null">
            AND idcard = #{idcard}
        </if>
    </select>

